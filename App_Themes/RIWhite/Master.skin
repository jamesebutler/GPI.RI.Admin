<asp:Menu Width="27%" SKINID="mainMenu" runat="server" Orientation="Horizontal"
            StaticEnableDefaultPopOutImage="true"
            StaticPopOutImageTextFormatString=""
            StaticPopOutImageUrl="~/Images/ArrowRight.gif"
            DynamicMenuItemStyle-ItemSpacing="3"
            StaticMenuItemStyle-ItemSpacing="6px"
            StaticMenuItemStyle-BackColor="#999999"
            StaticHoverStyle-BackColor="black">
    <StaticMenuStyle CssClass="mainMenuBack" />
    <StaticHoverStyle CssClass="mainMenuBackHover" />
    <StaticMenuItemStyle BorderStyle="solid" BorderWidth="0" BorderColor="white" />
    <DynamicMenuStyle CssClass="mainSubMenuBack" />
    <DynamicHoverStyle CssClass="mainSubMenuBackHover" />
    <DynamicMenuItemStyle CssClass="mainSubMenu"/>
    <DynamicSelectedStyle />


</asp:Menu>




